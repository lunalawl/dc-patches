   (uisink_signout_screen title_screen)
   (song_select_mode song_select_quickplay)
   (seldiff_screen multiuser_screen)
   (endgame_popup_between_screen perform_endgame_popup_between_screen)
   (multiuser_back
      {cond
         ({hamprovider get is_in_campaign_master_quest_mode}
            campaign_master_quest_song_select_screen)
         (TRUE song_select_screen)})
   (newsong_screen
      {cond
         ({hamprovider get is_in_campaign_mode}
            campaign_songselect_screen)
         ({hamprovider get is_in_campaign_master_quest_mode}
            campaign_master_quest_song_select_screen)
         (TRUE song_select_screen)}))
#define COMMON_ENTER_HANDLER
((enter
      {print
         "common enter handler for game mode "
         [gameplay_mode]
         "\n"}
      {hamprovider
         set
         requires_2_players
         [requires_2_players]}
      {hamprovider
         set
         use_movegraph
         [use_movegraph]}
      {hamprovider
         set
         use_single_move_detect
         [use_single_move_detect]}
      {hamprovider
         set
         merge_moves
         [merge_moves]}
      {hamprovider set hide_venue FALSE}
      {hamprovider
         set
         is_in_timeywimey
         [is_in_timeywimey]}
      {hamprovider
         set
         peak_behavior
         [peak_behavior]}
      {hamprovider
         set
         uses_score_star_display
         [uses_score_star_display]}
      {if_else
         [is_in_timeywimey]
         {handle
            (hamprovider enter_timeywimey)}
         {handle
            (hamprovider enter_nottimeywimey)}}
      {print
         "hamprovider get is_in_timeywimey = "
         {hamprovider get is_in_timeywimey}
         "\n"}))
(init COMMON_ENTER_HANDLER)
(perform_legacy
   (gameplay_mode perform_legacy)
   COMMON_ENTER_HANDLER)
(perform
   (gameplay_mode perform)
   (merge_moves TRUE)
   (use_movegraph TRUE)
   COMMON_ENTER_HANDLER)
(challenge
   (parent_mode perform)
   (multiuser_back
      {do
         {profile_mgr clear_critical_profile}
         {if_else
            {profile_mgr has_active_profile}
            {do
               ($profile
                  {profile_mgr get_active_profile})
               {if_else
                  {platform_mgr
                     is_pad_signed_into_live
                     {$profile get_pad_num}}
                  {if_else
                     {rock_central is_online}
                     {if_else
                        {challenges can_download_player_challenges}
                        downloading_player_challenge_screen
                        challenge_feed_screen}
                     {do
                        {downloading_player_challenge_screen set wait_until_rc_online TRUE}
                        downloading_player_challenge_screen}}
                  challenges_need_to_sign_in_screen}}
            challenges_need_to_sign_in_screen}})
   (gameplay_mode perform)
   (pause_screen challenge_perform_pause_screen)
   (newsong_screen challenge_feed_screen)
   COMMON_ENTER_HANDLER)
(practice
