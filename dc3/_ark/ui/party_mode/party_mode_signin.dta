                        {find_obj
                           {$this loaded_dir}
                           {sprint
                              "crewmate"
                              $i
                              "_left.mesh"}})
                     {$headshot_mesh set showing FALSE}}}}
            {crew_photo_left.mat
               set
               diffuse_tex
               {ui get_stored_texture 0}}
            {enter_announcement_left.flow activate}
            {start_scrolling_anims.flow activate}}
         {do
            {set
               $num_enrolled1
               {partymode_mgr num_enrolled_team_1}}
            {foreach_int
               $i
               0
               4
               {if_else
                  {< $i $num_enrolled1}
                  {do
                     ($headshot_mat
                        {find_obj
                           {$this loaded_dir}
                           {sprint
                              "crewmate"
                              $i
                              "_left.mat"}})
                     ($headshot_mesh
                        {find_obj
                           {$this loaded_dir}
                           {sprint
                              "crewmate"
                              $i
                              "_left.mesh"}})
                     {ui
                        apply_texture_clip
                        $headshot_mat
                        {partymode_mgr get_player_photo_index $i}}
                     {$headshot_mesh set showing TRUE}}
                  {do
                     ($headshot_mesh
                        {find_obj
                           {$this loaded_dir}
                           {sprint
                              "crewmate"
                              $i
                              "_left.mesh"}})
                     {$headshot_mesh set showing FALSE}}}}
            {crew_photo_left.mat
               set
               diffuse_tex
               {ui get_stored_texture 0}}
            {set
               $num_enrolled2
               {partymode_mgr num_enrolled_team_2}}
            {foreach_int
               $i
               0
               4
               {if_else
                  {< $i $num_enrolled2}
                  {do
                     ($headshot_mat
                        {find_obj
                           {$this loaded_dir}
                           {sprint
                              "crewmate"
                              $i
                              "_right.mat"}})
                     ($headshot_mesh
                        {find_obj
                           {$this loaded_dir}
                           {sprint
                              "crewmate"
                              $i
                              "_right.mesh"}})
                     {ui
                        apply_texture_clip
                        $headshot_mat
                        {partymode_mgr
                           get_player_photo_index
                           {'+' $num_enrolled1 $i}}}
                     {$headshot_mesh set showing TRUE}}
                  {do
                     ($headshot_mesh
                        {find_obj
                           {$this loaded_dir}
                           {sprint
                              "crewmate"
                              $i
                              "_right.mesh"}})
                     {$headshot_mesh set showing FALSE}}}}
            {crew_photo_right.mat
               set
               diffuse_tex
               {ui get_stored_texture 1}}
            {enter_announcement_right.flow activate}
            {start_scrolling_anims.flow activate}}}
      {{cursor_panel find depth_buffer_left.grp}
         set_showing
         FALSE}
      {{cursor_panel find depth_buffer_right.grp}
         set_showing
         FALSE}
      {meta music_start})
   (poll
      {if_else
         {!
