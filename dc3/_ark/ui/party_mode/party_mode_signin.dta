{new
   HamPanel
   party_mode_photos_panel
   (file
      "party_mode_photos.milo")
   (enter DX_NO_MENU_MOTIONBLUR
      {foreach_int
         $i
         1
         {'+' NUM_PHOTOS 1}
         {$this hide_photo $i}})
   (hide_photo
      ($idx)
      {do
         ($photo_mat)
         ($photo_mesh)
         {set
            $photo_mesh
            {find_obj
               {$this loaded_dir}
               {sprint
                  "p"
                  $idx
                  ".mesh"}}}
         {if
            $photo_mesh
            {$photo_mesh set_showing FALSE}}
         {set
            $photo_mesh
            {find_obj
               {$this loaded_dir}
               {sprint
                  "p"
                  $idx
                  "_flash.mesh"}}}
         {if
            $photo_mesh
            {$photo_mesh set_showing FALSE}}})
   (draw_photo
      ($idx)
      {do
         ($photo_mat)
         ($photo_mesh)
         {set
            $photo_mat
            {find_obj
               {$this loaded_dir}
               {sprint
                  "p"
                  $idx
                  ".mat"}}}
         {set
            $photo_mesh
            {find_obj
               {$this loaded_dir}
               {sprint
                  "p"
                  $idx
                  ".mesh"}}}
         {ui
            apply_texture_clip
            $photo_mat
            {- $idx 1}}
         {$photo_mesh set_showing TRUE}
         {set
            $photo_mesh
            {find_obj
               {$this loaded_dir}
               {sprint
                  "p"
                  $idx
                  "_flash.mesh"}}}
         {if
            $photo_mesh
            {$photo_mesh set_showing TRUE}}})
   (take_pictures
      {flash.flow activate})}
{new
   HamPanel
   party_mode_continue_panel
   (file
      "../low_button.milo")
   (focus right_hand.hnl)
   (helpbar_confirm_label yes)
   (show_hand_help FALSE)
   (is_enabled TRUE)
   (allow_voice_control FALSE)
   (callback_panel 0)
   (back_token no)
   (hide_letterbox FALSE)
   (allow_doubleuser_swipe TRUE)
   (disable
      {set
         [is_enabled]
         FALSE}
      {set
         [hide_helpbar]
         TRUE}
      {set
         [helpbar_allow_controller]
         FALSE}
      {hamprovider set ui_nav_mode shell_4p}
      {hamprovider set sided_colors_locked TRUE}
      {right_hand.hnl set showing FALSE}
      {helpbar resync})
   (enable
      {set
         [is_enabled]
         TRUE}
      {set
         [hide_helpbar]
         FALSE}
      {set
         [helpbar_allow_controller]
         TRUE}
      {hamprovider set ui_nav_mode shell}
      {hamprovider set sided_colors_locked FALSE}
      {right_hand.hnl
         set
         showing
         {!
            {shell_input in_controller_mode}}}
      {right_hand.hnl set_provider provider_ok.hnp}
      {helpbar resync})
   SELECT_RIGHT_LIST_TOKEN
   (poll SLIDE_RIGHT_LIST_ITEM)
   (BUTTON_DOWN_MSG
      {switch
         $action
         ((kAction_Cancel)
            {if
               {&&
                  {right_hand.hnl get showing}
                  [callback_panel]}
               {do
                  ($temp_callback_panel
                     [callback_panel])
                  {set
                     [callback_panel]
                     0}
                  {$this disable}
                  {$temp_callback_panel
                     continue_panel_callback
                     [back_token]}}})
         kDataUnhandled})
   (NAV_SELECT_MSG
      {if
         [callback_panel]
         {do
            ($temp_callback_panel
               [callback_panel])
            {set
               [callback_panel]
               0}
            {$this disable}
            {$temp_callback_panel continue_panel_callback $name}}})
   (controller_mode_entered
      {right_hand.hnl set showing FALSE})
   (controller_mode_exited
      {if
         [is_enabled]
         {right_hand.hnl set showing TRUE}})}
{new
   HamPanel
   party_mode_signin_panel
   (file
      "party_mode_signin.milo")
   (hide_helpbar TRUE)
   (helpbar_allow_controller FALSE)
   (back_token no)
   (hide_letterbox FALSE)
   (allow_doubleuser_swipe TRUE)
   (allow_voice_control FALSE)
   (skeleton_hand_up
