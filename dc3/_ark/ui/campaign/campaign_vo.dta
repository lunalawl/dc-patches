{func
   play_camp_vo
   ($vo_name $delay)
   {set
      $vo_obj
      ""}
   {set
      $vo_obj_name
      ""}
   {if
      $vo_name
      {if_else
         {==
            {type $vo_name}
            kDataObject}
         {set $vo_obj $vo_name}
         {do
            {set $vo_obj_name $vo_name}
            {set
               $vo_obj
               {find_obj
                  {$hamdirector get cur_world}
                  {sprint
                     $vo_name
                     ".snd"}}}
            {if
               {! $vo_obj}
               {set
                  $vo_obj
                  {find_obj
                     {$this loaded_dir}
                     {sprint
                        $vo_name
                        ".snd"}}}}}}}
   {if_else
      $vo_obj
      {do
         {print
            "play_camp_vo : "
            $vo_name
            "\n"}
         {if_else
            {== $delay 0}
            {$vo_obj play}
            {do
               {script_task
                  kTaskUISeconds
                  (delay $delay)
                  (script
                     {play_camp_vo $vo_obj_name 0})}}}}
      {print
         "play_camp_vo : "
         $vo_name
         " NOT FOUND!\n"}}}
{func
   trigger_camp_vo_shell_results_screen
   {print
      "trigger_camp_vo_shell_results_screen\n"}
   {if
      {==
         {gamemode get_mode}
         campaign_intro}
      {set
         $stars
         {game_panel get num_stars}}
      {set
         $index
         {meta_performer get_playlist_index}}
      {if
         {== $index 1}
         {cond
            ({>= $stars 5}
               {play_camp_vo camp_gameplay_song2_end56_cpu_01 4.4})
            ({>= $stars 3}
               {play_camp_vo camp_gameplay_song2_end34_cpu_01 4.1})
            ({>= $stars 0}
               {do
                  ($vo_flow
                     {find_obj
                        {$hamdirector get cur_world}
                        song2_fail_sequence.flow})
                  {$vo_flow activate}})}}}
   {if
      {'||'
         {==
            {gamemode get_mode}
            campaign_perform}
         {==
            {gamemode get_mode}
            campaign_holla_back}}
      {do
         ($cur_era
            {meta_performer get_era})
         ($cur_song
            {gamedata get song})
         ($is_craze_song
            {meta_performer is_attempting_dance_craze_song})
         {print
            "<perform> - song = "
            $cur_song
            ", is_craze="
            $is_craze_song
            ", era="
            $cur_era
            "\n"}
         {if
            {&&
               {== $cur_era era01}
               $is_craze_song}
            {play_camp_vo camp_gameplay_song7_win_cpu_01 4.4}}}}}
{func trigger_camp_vo_intro_start}
{func
   trigger_camp_vo_on_game_stage_playing
   {print
      "trigger_camp_vo_on_game_stage_playing\n"}
   {do
      ($is_campaign_perform
         {'||'
            {==
               {gamemode get_mode}
               campaign_perform}
            {==
               {gamemode get_mode}
               campaign_holla_back}})
      ($cur_era
         {meta_performer get_era})
      ($new_era
         {meta_performer is_era_new})
      ($is_craze_song
         {meta_performer is_attempting_dance_craze_song})
      ($cur_song
         {gamedata get song})
      ($song_index
         {meta_performer get_song_index $cur_song})
      ($is_era_intro_song
         {hamprovider get in_campaign_era_intro})
      ($vo_name
         "")
      {cond
         ({==
               {gamemode get_mode}
               campaign_intro}
            {print
               "{meta_performer get_playlist_index} = "
               {meta_performer get_playlist_index}
               "\n"}
            {if
               {==
                  {meta_performer get_playlist_index}
                  1}
               {play_camp_vo camp_gameplay_song2_start_cpu_01 3}})
         ({&& $is_campaign_perform $is_era_intro_song}
            {if
               {== $cur_era era01}
               {play_camp_vo camp_gameplay_song3_start_lima_01 3}})
         ({&& $is_campaign_perform $is_craze_song}
            {switch
               $cur_era
               (era01
                  {print
                     "we're not playing any VO here per BZ-62817, pages 16 & 17 of lazer script v24!\n"})
               (era02
                  {play_camp_vo camp_gameplay_song12_start_cpu_01 3})
               (era03
                  {play_camp_vo camp_gameplay_song17_start_cpu_01 3})
               (era04
                  {play_camp_vo camp_gameplay_song22_start_cpu_01 3})
               (era05
                  {play_camp_vo camp_gameplay_song26_start_cpu_01 3})})
         ({== $cur_era era01}
            {if_else
               {>
                  {meta_performer get_num_song_craze_moves $cur_song}
                  0}
               {play_camp_vo camp_gameplay_song4_start_power_lima_01 3}
               {play_camp_vo camp_gameplay_song4_nopower_lima_01 3}})}}}
{func
   trigger_camp_vo_on_game_stage_outro
