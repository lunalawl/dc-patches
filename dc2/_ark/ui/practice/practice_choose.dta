{new
   PracticeChoosePanel
   practice_choose_panel
   (file
      "practice_choose.milo")
   (focus right_hand.hnl)
   (back_token back)
   (load
      {game_panel load}
      {world_panel load})
   (unload
      {game_panel unload}
      {world_panel unload})
   (enter DX_NO_MENU_MOTIONBLUR
      {if
         $hamdirector
         {$hamdirector initialize}}
      {right_hand.hnl set_provider $this}
      {num_moves.anim set frame 0}
      {auto_selected.lbl
         set
         showing
         {$this want_to_auto_select_recommended}}
      {mini_marcos.grp set showing FALSE}
      {dc_logo.grp set showing TRUE}
      {hamprovider set ui_nav_mode practice_shell}
      {song.lbl
         set_song_name
         {gamedata get song}})
   (NAV_HIGHLIGHT_MSG
      {switch
         $component
         (right_hand.hnl
            {do
               ($num_moves
                  {$this
                     size
                     (step_moves $index moves)})
               ($start_beat
                  {$this step_start_beat $index})
               ($end_beat
                  {$this step_end_beat $index})
               ($song
                  {gamedata get song})
               ($difficulty
                  {gamedata
                     getp
                     {hamprovider get ui_nav_player}
                     difficulty})
               ($bps
                  {/
                     {{song_mgr get_meta_data $song}
                        bpm}
                     60})
               {num_moves.anim set frame $num_moves}
               {foreach_int
                  $move_index
                  0
                  $num_moves
                  {do
                     ($move
                        {$this
                           get
                           (step_moves $index moves $move_index)})
                     ($flashcard
                        {sprintf
                           "flashcard_%d"
                           {'+' $move_index 1}})
                     {$flashcard set_move $move TRUE}
                     {$flashcard set_award kSkillsAward_Unplayed TRUE FALSE}}}
               {if_else
                  {!= $start_beat -1}
                  {if_else
                     {$this step_is_recap $index}
                     {do
                        {mini_marcos.flow request_stop}
                        {mini_marcos.grp set showing FALSE}
                        {dc_logo.grp set showing TRUE}}
                     {do
                        {if_else
                           {== $num_moves 1}
                           {do
                              {set
                                 $first_move
                                 {{$this
                                       get
                                       (step_moves $index moves 0)}
                                    name}}
                              {search_replace
                                 $first_move
                                 ".move"
                                 ""
                                 $first_move}
                              {mini_marcos.iconman set move_name $first_move}
                              {mini_marcos.iconman set offset -0.75}
                              {minimarcos_anim_parameters
                                 set
                                 offset_time
                                 {/ 0.75 $bps}}}
                           {do
                              {mini_marcos.iconman
                                 set
                                 move_name
                                 ""}
                              {mini_marcos.iconman set start_beat $start_beat}
                              {mini_marcos.iconman set end_beat $end_beat}
                              {minimarcos_anim_parameters set offset_time 0}
                              {mini_marcos.iconman set offset 0}}}
                        {mini_marcos.iconman set difficulty $difficulty}
                        {minimarcos_anim_parameters
                           set
                           num_beats
                           {- $end_beat $start_beat}}
                        {minimarcos_anim_parameters
                           set
                           period
                           {/
                              {- $end_beat $start_beat}
                              $bps}}
                        {mini_marcos.flow activate}
                        {dc_logo.grp set showing FALSE}}}
                  {do
                     {mini_marcos.flow request_stop}
                     {mini_marcos.grp set showing FALSE}
                     {dc_logo.grp set showing TRUE}}}})})
   (NAV_SELECT_MSG
      {switch
         $component
         (right_hand.hnl
            {switch
               {$this step_start_beat $index}
               (-1
                  {if_else
                     {>
                        {$this num_selected}
                        0}
                     {do
                        {$this set_up_custom_section}
                        {hamprovider set byo_bid TRUE}
                        {if
                           {profile_mgr has_active_profile}
                           {acc_mgr
                              earn_accomplishment
                              {profile_mgr get_active_profile}
                              acc_bid_custom}}
                        {practice_welcome_panel set show_perform_shortcut TRUE}
                        {ui goto_screen game_screen}}
                     {do
                        {run_flow invalid_select.flow}
                        skip_select_anim_and_sound}})
               {do
                  ($selected
                     {$this
                        get
                        (step_moves $index selected)})
                  {$this
                     set
                     (step_moves $index selected)
                     {! $selected}}
                  {right_hand.hnl refresh}
                  skip_select_anim}})
         (left_hand.hnl
            {switch
               $name
               (back
                  {ui
                     go_back_screen
                     {{ui current_screen}
                        get
                        back}})})})}
{new
   HamScreen
   practice_choose_screen
   (focus practice_choose_panel)
   (panels background_panel practice_choose_panel)
   (back practice_welcome_screen)}