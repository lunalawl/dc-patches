{new
   MainMenuPanel
   main_panel
   (file
      "main.milo")
   (focus right_hand.hnl)
   (back_token back)
   (allow_voice_control TRUE)
   (enter DX_NO_MENU_MOTIONBLUR
      {platform_mgr
         add_sink
         $this
         (connection_status_changed)}
      {memcardmgr
         add_sink
         $this
         (save_load_all_complete_msg)}
      {player_provider_1
         add_sink
         $this
         (padnum_updated)}
      {player_provider_2
         add_sink
         $this
         (padnum_updated)}
      {hamprovider set ui_nav_mode shell}
      {profile_mgr clear_critical_profile}
      {depth_buffer enable_alert_feed}
      {meta music_start}
      {song_offer_provider stop_preview}
      {achievements set_allow_achievements TRUE}
      {song_offer_provider clear_saved_highlight}
      {gamemode set_mode init}
      {$this setup_messages motd.lbl}
      {content_mgr start_refresh}
      {hamprovider set voice_commander_tip_lower FALSE})
   (exit
      {platform_mgr remove_sink $this connection_status_changed}
      {memcardmgr remove_sink $this}
      {player_provider_1 remove_sink $this padnum_updated}
      {player_provider_2 remove_sink $this padnum_updated})
   (NAV_SELECT_MSG
      {switch
         $name
         (gameplay
            {gamemode set_mode init}
            {ui goto_screen song_select_screen})
         (campaign
            {gamemode set_mode campaign}
            {ui goto_screen campaign_choose_profile_screen})
         (fitness_mode
            {ui goto_screen fitness_screen})
         (options
            {ui goto_screen options_screen})
         (store
            {if_else
               {profile_mgr has_active_profile_no_override}
               {do
                  ($profile
                     {profile_mgr get_active_profile_no_override})
                  {if_else
                     {platform_mgr
                        is_pad_signed_into_live
                        {$profile get_pad_num}}
                     {do
                        {profile_mgr set_critical_profile $profile}
                        {ui goto_screen store_loading_screen}}
                     {ui goto_screen show_offers_need_to_sign_in_screen}}}
               {ui goto_screen show_offers_need_to_sign_in_screen}})
         (back
            {ui
               go_back_screen
               {{ui current_screen}
                  get
                  back}})})
   (motd_loaded
      {$this setup_messages motd.lbl})
   (save_load_all_complete_msg
      {$this setup_messages motd.lbl})
   (padnum_updated
      {$this setup_messages motd.lbl})}
{new
   HamScreen
   main_screen
   (focus main_panel)
   (panels meta background_panel main_panel)
   (back title_screen)
   (TRANSITION_COMPLETE_MSG
      {profile_mgr purge_old_data}
      {if
         {&&
            {!
               {ui_event_mgr has_active_transition_event}}
            {!
               {ui in_transition}}
            {==
               {ui current_screen}
               $this}}
         {saveload_mgr autosave}}
      kDataUnhandled)}
{new
   HamPanel
   main_gameplay_panel
   (file
      "main_gameplay.milo")
   (focus right_hand.hnl)
   (back_token back)
   (NAV_SELECT_MSG
      {switch
         $name
         (quickplay
            {gamemode set_mode init}
            {ui goto_screen song_select_screen})
         (campaign
            {gamemode set_mode campaign}
            {ui goto_screen campaign_diffselect_screen})
         (back
            {ui
               go_back_screen
               {{ui current_screen}
                  get
                  back}})})}
{new
   HamScreen
   main_gameplay_screen
   (focus main_gameplay_panel)
   (panels meta background_panel main_gameplay_panel)
   (back main_screen)}
{new
   MoviePanel
   attract_movie_panel
   (preload FALSE)
   (loop FALSE)
   (audio TRUE)
   (videos attract)
   (movie_done
      {if
         {==
            {ui current_screen}
            attract_screen}
         {ui
            goto_screen
            {{ui current_screen}
               get
               next_screen}}})}
{new
   HamScreen
   attract_screen
   (panels meta attract_movie_panel movie_overlay_panel)
   (focus movie_overlay_panel)
   (next_screen autosave_warning_screen)
   (enter DX_NO_MENU_MOTIONBLUR
      {profile_mgr set_overscan TRUE}) ; dx - enable overscan by default
   (skip_selected
      {ui goto_screen autosave_warning_screen})}
{new
   HamScreen
   show_offers_need_to_sign_in_screen
   (panels meta background_panel dialog_panel)
   (focus dialog_panel)
   (enter DX_NO_MENU_MOTIONBLUR
      {dialog_panel set_ok show_offers_need_to_sign_in})
   (NAV_SELECT_MSG
      {ui goto_screen main_screen})}
{new
   ChooseProfilePanel
   campaign_choose_profile_panel
   (file
      "..\\title\\choose_profile.milo")
   (focus right_hand.hnl)
   (back_token back)
   (enter DX_NO_MENU_MOTIONBLUR
      {right_hand.hnl set_provider $this})
   (refresh_ui
      {right_hand.hnl refresh})
   (NAV_SELECT_MSG
      {switch
         $component
         (right_hand.hnl
            {if_else
               {$this profile_selected $index}
               {do
                  ($profile
                     {$this get_profile $index})
                  {profile_mgr set_critical_profile $profile}
                  {ui goto_screen campaign_diffselect_screen}}
               {do
                  {$this show_signin}
                  skip_select_anim}})
         (left_hand.hnl
            {switch
               $name
               (back
                  {ui
                     go_back_screen
                     {{ui current_screen}
                        get
                        back}})})})}
{new
   HamScreen
   campaign_choose_profile_screen
   (panels meta background_panel campaign_choose_profile_panel)
   (focus campaign_choose_profile_panel)
   (back main_screen)}