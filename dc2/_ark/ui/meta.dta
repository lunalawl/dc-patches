{new
   VoiceControlPanel
   voice_control_panel
   (file
      "voice/voice_commander.milo")
   (show_hand_help FALSE)
   (back_token back)
   (helpbar_confirm_label none)
   (song_name '')
   (enter DX_NO_MENU_MOTIONBLUR)
   (exit)
   (set_song
      ($song)
      {set
         [song_name]
         $song}
      {song.lbl
         set_song_name
         [song_name]}
      {album.lbl
         set_album_name
         [song_name]}
      {artist.lbl
         set_artist_name
         [song_name]}
      {$this refresh_album_art})
   (refresh_album_art
      {if_else
         {!=
            [song_name]
            ''}
         {do
            ($content_name
               {song_mgr
                  content_name_from_sym
                  [song_name]})
            {if_else
               {'||'
                  {==
                     $content_name
                     ""}
                  {content_mgr is_mounted $content_name}}
               {do
                  ($pic
                     {album_art find album_art.pic})
                  ($mesh
                     {album_art find album.mesh})
                  ($mesh_back
                     {album_art find album_black_bg.mesh})
                  {$pic
                     set
                     tex_file
                     {song_mgr
                        album_art_path
                        [song_name]}}
                  {$mesh set showing TRUE}
                  {$mesh_back set showing TRUE}}
               {do
                  ($mesh
                     {album_art find album.mesh})
                  ($mesh_back
                     {album_art find album_black_bg.mesh})
                  {$mesh set showing FALSE}
                  {$mesh_back set showing FALSE}}}}
         {{album_art find album.mesh}
            set
            showing
            FALSE}})
   (NAV_SELECT_MSG
      {switch
         $name
         (back
            {$this dismiss})})
   (BUTTON_DOWN_MSG
      {switch
         $action
         (kAction_Cancel
            {$this dismiss}
            0)})}
